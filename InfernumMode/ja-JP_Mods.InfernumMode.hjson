Items: {
	EnrageTooltip: 憤怒
	DeveloperItem: "[c/{0}:~ Developer Item ~]"
	InfernalRelicText: 地獄の炎に包まれている

	Lore: {
		KnowledgeBereftVassal: {
			Lore:
				'''
				孤独な仲間意識を持つ二人組。
				かつて高名な戦士だったアルガスは、イルメリスの廃墟から出てきた数少ない生き残りの一人だったようだ。
				目的もなく、彼はこの謎めいた廃墟に居座り、あなたが到着するまで、静かに時の流れを観測していた。
				彼の辿った運命は、誤った独善の結果だ。大義を追い求めるあまり、無意味な苦痛を与えてはならない。
				'''
			DisplayName: 遺された臣下
		}
	}

	CelestialSigil: {
