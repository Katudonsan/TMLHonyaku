			UWUSlime: {
				BossChecklistIntegration: {
					SpawnInfo: Use [i:6509] in day.
					DespawnMessage: Slimes have taken their revenge
					EntryName: Slimes Last Hope
				}

				DisplayName: U W U Slime
			}

			LavalGolem: {
				DisplayName: Laval Golem

				BossChecklistIntegration: {
					SpawnInfo: Use [i:6511] in underworld.
					DespawnMessage: Fallen souls return to sleep
				}
			}

			DrFetusFirstPhase: {
				DisplayName: Dr Fetus First Phase

				BossChecklistIntegration: {
					EntryName: Dr Fetus
					SpawnInfo: Use [i:6519] which have a chance to drop from enemies in corruption or crimson if Dr Fetus haven't defeated. After it's defeat it can be bought from dark huntress.
					DespawnMessage: Players have been eliminated by Dr Fetus
				}
			}

			FlagCarrier: {
				DisplayName: Flag Carrier

				BossChecklistIntegration: {
					EntryName: Ancient Trio
					SpawnInfo: Use [i:6485] at night.
					DespawnMessage: All players have been devoured by the beasts
				}
			}

			SATLA001: {
				BossChecklistIntegration: {
					DespawnMessage: Satla have broken all of its toys
					SpawnInfo: Use [i:6503] in night.
					EntryName: SATLA-001
				}

				DisplayName: S A T L A001
			}

			SlayerofEvil: {
				BossChecklistIntegration: {
					DespawnMessage: All villains have been exterminated by the hero
					SpawnInfo: Bound dark huntress have a chance to spawn on surface layer before 1.30 PM and click to her help button will spawn him or he can be spawned with [i:6506] which have a chance to drop from Dryad. Also when you defeat him this deactivates and you can spawn him with [i:6506] which can be bought from dark huntress
					EntryName: Slayer of Evil
				}

				DisplayName: Slayerof Evil
			}

			HeroicSlime: {
				DisplayName: Heroic Slime

				BossChecklistIntegration: {
					EntryName: Slime Troop Tier 1 Miniboss
					SpawnInfo: At the end of slime troop event which can be summoned with [i:6510] one of them will summon with respect to world evil. After it's defeat use [i:6492] or [i:6497] to summon respected miniboss which can be bought from dark huntress.
					DespawnMessage: Slime troop clear the obstacle
				}
			}

			StormDiver: {
				DisplayName: Storm Diver

				BossChecklistIntegration: {
					EntryName: Royal Harpy Air Force
					SpawnInfo: It have low chance to spawn on air in day after EoW/BoC have been defeated or they can be summoned by [i:6507] which have low chance to drop from harpies if they haven't defeated. After it's defeat [i:6507] can be bought from dark huntress.
					DespawnMessage: Royal Harpy Air Force complete the mission
				}
			}

			Isaac: {
				DisplayName: Isaac

				BossChecklistIntegration: {
					SpawnInfo: It have low chance to spawn in underground after EoW/BoC have been defeated. After it's defeat use [i:6516] which can be bought from dark huntress.
					DespawnMessage: Isaac is now left with his inner demons
				}
			}

			Okiku: {
				DisplayName: Okiku

				BossChecklistIntegration: {
					SpawnInfo: It have a low chance to spawn in blood moon after EoC have been defeated or by [i:6486] which have a chance to drop from dripplers and blood zombies if she is not defeated. After it's defeat [i:6486] can be bought from dark huntress.
					DespawnMessage: Okiku's soul start to seek new sinners
				}
			}

			NecromancerDummy: {
				DisplayName: Necromancer Dummy

				BossChecklistIntegration: {
					EntryName: Antony, the Novice Necromancer
					SpawnInfo: It have a rare chance to spawn in surface at night or can be summoned with [i:6515]. After it's defeat [i:6515] can be bought from dark huntress.
					DespawnMessage: Antony accidentally kill all of the players
				}
			}

			BunnyZepplin: {
				DisplayName: Bunny Zepplin

				BossChecklistIntegration: {
					EntryName: Bunny Zeppelin
					SpawnInfo: It have a low chance to spawn from death of bunnies in day after EoC have been defeated or can be summoned with [i:6518]. After it's defeat [i:6518] can be bought from dark huntress.
					DespawnMessage: Bunny Zeppelin collects all burnt heads for their mighty king
				}
			}

			SecurityBot: {
				DisplayName: Security Bot

				BossChecklistIntegration: {
					EntryName: Tier 1 Dungeon Miniboss
					SpawnInfo: It have a low chance to spawn in dungeon with respect to world evil. They could also summon with their respective summon item. After their defeat summon item of both of them can be bought from dark huntress.
					DespawnMessage: Dungeon take down the threat
				}
			}

			IceGuardian: {
				DisplayName: Ice Guardian

				BossChecklistIntegration: {
					EntryName: Guardian of Frost
					SpawnInfo: He will spawn at the end of the frost legion. After his defeat use [i:6498] which can be bought from dark huntress.
					DespawnMessage: Guardian of frost eliminate the main reason of global warming
				}
			}

			NightmareAmplifierSlime: {
				DisplayName: Nightmare Amplifier Slime

				BossChecklistIntegration: {
					EntryName: Slime Troop Tier 2 Miniboss
					SpawnInfo: They will spawn at the end of the slime troop events tier 2 which activate after slimes last hope have been defeated. After their defeat use [i:6500] or [i:6508] which can be bought from dark huntress.
					DespawnMessage: Slimes gain their honor back
				}
			}

			AncientGuardian: {
				DisplayName: Ancient Guardian

				BossChecklistIntegration: {
					EntryName: The Ancient Guardian
					SpawnInfo: It have low chance to spawn in snow biome after EoW/BoC have been defeated. After it's defeat use [i:6505] which can be bought from dark huntress.
					DespawnMessage: The Ancient Guardian return back to wait for its masters return
				}
			}

